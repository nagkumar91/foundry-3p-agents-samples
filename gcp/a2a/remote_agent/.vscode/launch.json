{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Currency Agent",
      "type": "debugpy",
      "request": "launch",
      "module": "remote_agent",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/gcp/a2a"
      },
      "cwd": "${workspaceFolder}/gcp/a2a/remote_agent",
      "args": ["--host", "localhost", "--port", "10000"]
    }
  ]
}
